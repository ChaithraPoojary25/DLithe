void setup() {
 
  pinMode(42, OUTPUT);
  pinMode(6, OUTPUT);
  Serial.begin(9600);
}
void loop() {
  digitalWrite(42, HIGH);  
  digitalWrite(6, HIGH);  
  Serial.println("led is on");
  delay(1000);  
  digitalWrite(42, LOW); 
  digitalWrite(6,LOW);
  Serial.println("led is off");
  delay(1000);                      
}
